  "main": "index.js",
  "scripts": {
    "serve": "serve dist",
    "start": "node_modules/.bin/webpack-dev-server --host 0.0.0.0  --config webpack.prod.js",
    "start:development": "node_modules/.bin/webpack-dev-server --host 0.0.0.0 --config webpack.dev.js",
    "start:production": "node_modules/.bin/webpack-dev-server --config webpack.prod.js",
    "start:analyze": "webpack-dev-server --env.mode development --env.analyze",
    "build": "./node_modules/.bin/cross-env NODE_ENV=development ./node_modules/.bin/webpack --config webpack.dev.js",
    "build:dev": "rimraf dist && cross-env NODE_ENV=development webpack  --config webpack.dev.js",
    "build:prod": "rimraf dist && cross-env NODE_ENV=production webpack  --config webpack.prod.js",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint:fix": "eslint --fix src/**/*.{js,jsx} tests/**/*.js  cypress/**/*.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:all": "set CI=true && yarn test && yarn lint",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "start:storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook"
  },
